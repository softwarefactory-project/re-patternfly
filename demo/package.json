{
  "name": "re-patternfly-seed",
  "version": "1.0.0",
  "description": "A starter project for re-patternfly",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "test": "jest --verbose --testMatch **/tests/*.bs.js",
    "serve": "parcel serve ./src/index.html",
    "dist": "parcel build ./src/index.html --public-url /"
  },
  "keywords": [
    "ReasonReact",
    "patternfly",
    "reason",
    "reason-react",
    "reasonml",
    "reasonml-bindings",
    "bucklescript",
    "bucklescript-bindings"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@patternfly/react-core": "^4.50.2",
    "@patternfly/react-icons": "^4.7.12",
    "@patternfly/react-table": "^4.18.14",
    "@softwarefactory-project/re-patternfly": "..",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-platform": "^8.2.0",
    "parcel": "^1.12.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/styleMock.js"
    }
  },
  "main": "index.js"
}
